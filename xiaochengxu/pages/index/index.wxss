.page__hd{
  width: 100%;
  height: 130rpx;
  background-color: #F788AB;
}
.page__hd_search{
  width: 540rpx;
  height: 80rpx;
  background: #f74a81;
  border-radius: 40rpx;
  font-size: 32rpx;
  color: #ffffff;
}
.page__hd_search image{
  width: 38rpx;
  height: 44rpx;
  margin-left: 10rpx;
}
.swiper-tab{
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-bottom: 1rpx solid #ededed;
}
.swiper-tab-item{
  flex: 1;
  text-align: center;
  font-size: 32rpx;
  height: 90rpx;
  color: #999999;
  line-height: 90rpx;
}
.active{
  color: #f65959;
}
swiper{
  text-align: center;
  display: flex;
  justify-content: center;
  align-content: center;
  height: 800rpx; 
}
swiper-item{
  overflow-y:scroll;
}
